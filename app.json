{
    "expo": {
        "name": "react-native-fiber-firebase",
        "version": "1.1.6",
        "description": "UI elements, multi-screen implemented in React Native, now in one handy kit",
        "sdkVersion": "33.0.0",
        "orientation": "portrait",
        "privacy": "public",
        "icon": "./assets/app.png",
        "primaryColor": "#00AAFF",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#ffffff"
        },
        "packagerOpts": {
            "assetExts": ["otf", "ttf"]
        },
        "androidStatusBar": {
            "backgroundColor": "#ffffff",
            "barStyle": "dark-content"
        },
        "ios": {
            "bundleIdentifier": "com.wcandillon.fiber",
            "buildNumber": "2"
        },
        "android": {
            "package": "com.wcandillon.fiber"
        },
        "assetBundlePatterns": ["assets/*"]
    }
}
